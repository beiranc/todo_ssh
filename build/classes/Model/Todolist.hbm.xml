<?xml version="1.0" encoding="UTF-8"?>

<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">

	<class name="Model.Todolist" table="todolist">
		<id name="todolistId" column="todolistId" type="java.lang.String">
			<generator class="assigned"/>
		</id>
		<property name="title" column="title" type="java.lang.String"></property>
		<property name="comments" column="comments" type="java.lang.String"></property>
		<property name="userId" column="userId" type="java.lang.String"></property>
		<property name="is_del" column="is_del" type="java.lang.Integer"></property>
	</class>
	
</hibernate-mapping>